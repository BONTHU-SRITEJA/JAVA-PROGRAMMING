import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int[] a= new int[10];
        for(int i=0;i<10;i++)
            a[i]=in.nextInt();
        int min=a[0];
        for(int i=1;i<10;i++)
            if(a[i]<min)
                min=a[i];
        System.out.println("Thank you! "+min+" is the smallest number.");
    }
}
